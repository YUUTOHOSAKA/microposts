<h2>フォロワー一覧ページ</h2>
<div class="panel panel-default">
  <div class="panel-heading">
    フォロワー一覧
  </div>

  <div class="panel-body">
    <% @relationships.each do |relationship| %>
      <span class="label label-info"><%= relationship.name %></span>
      <%= relationship.email %>
      <br>
    <% end %>
  </div>
</div>
